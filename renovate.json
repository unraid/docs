{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:allNonMajor"
  ],
  "postUpdateOptions": [
    "pnpmDedupe"
  ],
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "groupName": "docusaurus monorepo",
      "rangeStrategy": "bump",
      "matchPackageNames": [
        "/docusaurus/"
      ]
    }
  ]
}
