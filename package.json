{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "remark . --ext .md,.mdx --quiet --rc-path .remarkrc.mjs",
    "lint:fix": "remark . --ext .md,.mdx --output --rc-path .remarkrc.mjs",
    "format:crowdin": "node scripts/format-crowdin-spacing.js",
    "format": "npm run lint:fix && npm run format:crowdin",
    "test": "vitest",
    "test:run": "vitest run",
    "prettier": "prettier --write docs",
    "parse-terms": "docusaurus-terminology parse --dry-run",
    "check-case-sensitivity": "node scripts/check-case-sensitivity.js",
    "prebuild": "npm run check-case-sensitivity",
    "postinstall": "npm rebuild sharp",
    "crowdin:upload": "npm run format && crowdin upload --config crowdin.yml",
    "crowdin:sync": "docusaurus write-translations && npm run format && crowdin upload && crowdin download && node scripts/format-crowdin-spacing.js",
    "crowdin:download": "crowdin download --config crowdin.yml && node scripts/format-crowdin-spacing.js",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@crowdin/cli": "^4.11.0",
    "@docusaurus/core": "3.8.1",
    "@docusaurus/plugin-client-redirects": "^3.7.0",
    "@docusaurus/plugin-ideal-image": "^3.3.2",
    "@docusaurus/preset-classic": "3.8.1",
    "@grnet/docusaurus-terminology": "^2.0.0-rc.2",
    "@lunaticmuch/docusaurus-terminology": "^1.5.0",
    "@mdx-js/react": "^3.0.1",
    "@renatonagliati/remark-auto-glossary": "github:renatonagliati/remark-auto-glossary",
    "clsx": "^2.1.1",
    "docusaurus-plugin-image-zoom": "^3.0.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-tooltip": "^5.29.1",
    "semver": "^7.6.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/tsconfig": "^3.3.2",
    "@docusaurus/types": "^3.3.2",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^19.0.0",
    "glob": "^11.0.0",
    "prettier": "^3.2.5",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-lint-code-block-style": "^4.0.1",
    "remark-lint-definition-case": "^4.0.1",
    "remark-lint-fenced-code-marker": "^4.0.1",
    "remark-lint-heading-style": "^4.0.1",
    "remark-lint-link-title-style": "^4.0.1",
    "remark-lint-list-item-indent": "^4.0.1",
    "remark-lint-maximum-line-length": "^4.1.1",
    "remark-lint-no-duplicate-definitions": "^4.0.1",
    "remark-lint-no-file-name-outer-dashes": "^3.0.1",
    "remark-lint-no-heading-punctuation": "^4.0.1",
    "remark-lint-no-multiple-toplevel-headings": "^4.0.1",
    "remark-lint-no-shell-dollars": "^4.0.1",
    "remark-lint-no-undefined-references": "^5.0.2",
    "remark-lint-no-unused-definitions": "^4.0.2",
    "remark-lint-ordered-list-marker-style": "^4.0.1",
    "remark-mdx": "^3.1.1",
    "remark-preset-lint-recommended": "^7.0.1",
    "typescript": "^5.4.5",
    "unist-util-visit": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "optionalDependencies": {
    "sharp": "^0.34.3"
  },
  "overrides": {
    "sharp": "^0.34.3"
  }
}
