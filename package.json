{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "remark . --ext .md,.mdx --quiet --rc-path .remarkrc.mjs",
    "format:crowdin": "node scripts/format-crowdin-spacing.js",
    "format": "npm run format:crowdin",
    "test": "vitest",
    "test:run": "vitest run",
    "prettier": "prettier --write docs",
    "check-case-sensitivity": "node scripts/check-case-sensitivity.js",
    "prebuild": "npm run check-case-sensitivity",
    "postinstall": "npm rebuild sharp",
    "crowdin:upload": "npm run format && crowdin upload --config crowdin.yml",
    "crowdin:sync": "docusaurus write-translations && npm run format && crowdin upload && crowdin download && npm run format:crowdin",
    "crowdin:download": "crowdin download --config crowdin.yml && npm run format:crowdin",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
