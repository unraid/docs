{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "npm run check-case-sensitivity && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint:md": "markdownlint-cli2-fix docs/**/**/*.md",
    "prettier": "prettier --write docs",
    "parse-terms": "docusaurus-terminology parse --dry-run",
    "check-case-sensitivity": "node scripts/check-case-sensitivity.js",
    "prebuild": "npm run check-case-sensitivity",
    "postinstall": "npm rebuild sharp",
    "crowdin:upload": "crowdin upload --config crowdin.yml",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@crowdin/cli": "^4.11.0",
    "@docusaurus/core": "3.7.0",
    "@docusaurus/plugin-client-redirects": "^3.7.0",
    "@docusaurus/plugin-ideal-image": "^3.3.2",
    "@docusaurus/preset-classic": "3.7.0",
    "@grnet/docusaurus-terminology": "^2.0.0-rc.2",
    "@lunaticmuch/docusaurus-terminology": "^1.5.0",
    "@mdx-js/react": "^3.0.1",
    "@renatonagliati/remark-auto-glossary": "github:renatonagliati/remark-auto-glossary",
    "clsx": "^2.1.1",
    "docusaurus-plugin-image-zoom": "^3.0.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-tooltip": "^5.29.1",
    "semver": "^7.6.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/tsconfig": "^3.3.2",
    "@docusaurus/types": "^3.3.2",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^19.0.0",
    "glob": "^10.3.10",
    "markdownlint-cli2": "^0.18.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "optionalDependencies": {
    "sharp": "^0.34.3"
  },
  "overrides": {
    "sharp": "^0.34.3"
  }
}
